<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <data>
    <record id="scenario_tree" model="ir.ui.view">
      <field name="name">scenario.tree</field>
      <field name="model">scenario</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name" />
          <field name="end_date" />
        </tree>
      </field>
    </record>

    <record id="scenario_action" model="ir.actions.act_window">
      <field name="name">Scenario</field>
      <field name="res_model">scenario</field>
      <field name="type">ir.actions.act_window</field>
      <field name="view_mode">tree</field>
    </record>
  </data>
</odoo>
