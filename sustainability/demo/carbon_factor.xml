<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Achat de biens -->
    <record id="carbon_factor_achats_de_biens" model="carbon.factor">
        <field name="name">Achats de biens</field>
        <field name="carbon_compute_method" />
        <field name="uncertainty_percentage">0</field>
    </record>
